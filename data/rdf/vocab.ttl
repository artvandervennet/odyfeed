@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix dct:  <http://purl.org/dc/terms/>.
@prefix as:   <https://www.w3.org/ns/activitystreams#>.
@prefix myth: <https://odyfeed.artvandervennet.ikdoeict.be/vocab#>.

myth:Actor
    a          rdfs:Class ;
    rdfs:label "Mythological Actor" ;
    rdfs:comment "An actor from mythological narratives, acting as a bot in the OdyFeed system." ;
    owl:equivalentClass as:Service .

myth:Event
    a          rdfs:Class ;
    rdfs:label "Narrative Event" ;
    rdfs:comment "A specific event in a mythological narrative." ;
    owl:equivalentClass as:Note .

myth:sequence
    a           rdf:Property ;
    rdfs:label  "sequence number" ;
    rdfs:domain myth:Event ;
    rdfs:range  xsd:integer .

myth:description
    a           rdf:Property ;
    rdfs:label  "description" ;
    rdfs:domain myth:Event ;
    rdfs:range  xsd:string ;
    rdfs:subPropertyOf as:content .

myth:involvesActor
    a           rdf:Property ;
    rdfs:label  "involves actor" ;
    rdfs:domain myth:Event ;
    rdfs:range  myth:Actor ;
    rdfs:subPropertyOf as:attributedTo .

myth:tone
    a           rdf:Property ;
    rdfs:label  "narrative tone" ;
    rdfs:domain myth:Actor ;
    rdfs:range  xsd:string .

myth:avatar
    a           rdf:Property ;
    rdfs:label  "avatar image" ;
    rdfs:domain myth:Actor ;
    rdfs:range  xsd:anyURI ;
    rdfs:subPropertyOf as:image .